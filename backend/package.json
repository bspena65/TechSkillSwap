{
  "name": "techskillback",
  "version": "1.4.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "dev": "nodemon --exec ts-node src/main.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "test": "jest --passWithNoTests",
    "cleanpackage": "rm -rf node_modules && rm -rf package-lock.json yarn.lock && yarn install",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/infrastructure/persistence/typeormSource.ts"
