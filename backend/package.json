    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "fs-extra": "^11.2.0",
    "googleapis": "^144.0.0",
    "helmet": "^7.1.0",
    "ics": "^3.8.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
